{
  "name": "svelte-snake-workshop",
  "version": "2.0.0",
  "scripts": {
    "build": "svelte-kit build",
    "dev": "svelte-kit dev",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "generate-translations": "md2po2md README.md -l nob -o \"locale/{lang}\" && md2po2md TASKS.md -l nob -o \"locale/{lang}\" && md2po2md SETUP.md -l nob -o \"locale/{lang}\"",
    "postgenerate-translations": "npm run format",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^4.3.0",
    "lint-staged": "^12.1.2",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0"
  },
  "type": "module"
}
